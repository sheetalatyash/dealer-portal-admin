{
  "name": "user-administration",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "ua",
  "sourceRoot": "apps/user-administration/src",
  "tags": ["app", "ui"],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/user-administration",
        "index": "apps/user-administration/src/index.html",
        "browser": "apps/user-administration/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/user-administration/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/user-administration/public"
          },
          {
            "glob": "**/*",
            "input": "dist/packages/dealer-portal-theme/css",
            "output": "css"
          },
          {
            "glob": "**/*",
            "input": "dist/packages/dealer-portal-theme/fonts",
            "output": "fonts"
          },
          {
            "glob": "**/*",
            "input": "dist/packages/dealer-portal-theme/images",
            "output": "images"
          },
          {
            "glob": "**/*",
            "input": "dist/packages/dealer-portal-theme/js",
            "output": "js"
          },
          {
            "glob": "**/*",
            "input": "apps/user-administration/src/assets",
            "output": "."
          }
        ],
        "styles": ["apps/user-administration/src/styles.scss"],
        "scripts": [],
        "allowedCommonJsDependencies": [
          "ua-parser-js",
          "reflect-metadata"
        ],
        "budgets": [
            {
              "type": "initial",
              "maximumWarning": "5mb",
              "maximumError": "10mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "5kb",
              "maximumError": "150kb"
            }
          ]
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/user-administration/src/_environments/environment.ts",
              "with": "apps/user-administration/src/_environments/environment.production.ts"
            }
          ]
        },
        "staging": {
          "fileReplacements": [
            {
              "replace": "apps/user-administration/src/_environments/environment.ts",
              "with": "apps/user-administration/src/_environments/environment.staging.ts"
            }
          ]
        },
        "development": {
          "fileReplacements": [
            {
              "replace": "apps/user-administration/src/_environments/environment.ts",
              "with": "apps/user-administration/src/_environments/environment.development.ts"
            }
          ]
        },
        "local": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "fileReplacements": [
            {
              "replace": "apps/user-administration/src/_environments/environment.ts",
              "with": "apps/user-administration/src/_environments/environment.local.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "user-administration:build:production"
        },
        "staging": {
          "buildTarget": "user-administration:build:staging"
        },
        "development": {
          "buildTarget": "user-administration:build:development"
        },
        "local": {
          "buildTarget": "user-administration:build:local"
        }
      },
      "defaultConfiguration": "local"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "user-administration:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/user-administration/jest.config.ts"
      }
    }
  }
}
