{
  "name": "communications",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "comm",
  "sourceRoot": "apps/communications/src",
  "tags": [
    "app",
    "ui"
  ],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/communications",
        "index": "apps/communications/src/index.html",
        "browser": "apps/communications/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/communications/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/communications/public"
          },
          {
            "glob": "**/*",
            "input": "dist/packages/dealer-portal-theme/css",
            "output": "css"
          },
          {
            "glob": "**/*",
            "input": "dist/packages/dealer-portal-theme/fonts",
            "output": "fonts"
          },
          {
            "glob": "**/*",
            "input": "dist/packages/dealer-portal-theme/images",
            "output": "images"
          },
          {
            "glob": "**/*",
            "input": "dist/packages/dealer-portal-theme/js",
            "output": "js"
          },
          {
            "glob": "**/*",
            "input": "apps/communications/src/assets",
            "output": "."
          }
        ],
        "styles": [
          "apps/communications/src/styles.scss"
        ],
        "scripts": [],
        "allowedCommonJsDependencies": [
          "ua-parser-js",
          "reflect-metadata"
        ],
        "budgets": [
          {
            "type": "initial",
            "maximumWarning": "5mb",
            "maximumError": "10mb"
          },
          {
            "type": "anyComponentStyle",
            "maximumWarning": "5kb",
            "maximumError": "150kb"
          }
        ]
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/communications/src/_environments/environment.ts",
              "with": "apps/communications/src/_environments/environment.production.ts"
            }
          ]
        },
        "staging": {
          "fileReplacements": [
            {
              "replace": "apps/communications/src/_environments/environment.ts",
              "with": "apps/communications/src/_environments/environment.staging.ts"
            }
          ]
        },
        "development": {
          "fileReplacements": [
            {
              "replace": "apps/communications/src/_environments/environment.ts",
              "with": "apps/communications/src/_environments/environment.development.ts"
            }
          ]
        },
        "local": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "fileReplacements": [
            {
              "replace": "apps/communications/src/_environments/environment.ts",
              "with": "apps/communications/src/_environments/environment.local.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "communications:build:production"
        },
        "staging": {
          "buildTarget": "communications:build:staging"
        },
        "development": {
          "buildTarget": "communications:build:development"
        },
        "local": {
          "buildTarget": "communications:build:local"
        }
      },
      "defaultConfiguration": "local"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "communications:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/communications/jest.config.ts"
      }
    }
  }
}
