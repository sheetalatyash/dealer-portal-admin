{
  "name": "@dealer-portal/source",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "reset": "nx reset",
    "run:all:apps": "nx run-many --all -t serve",
    "build:all": "rimraf dist && nx run-many --all -t build --verbose",
    "build:all:local": "nx run-many --all -t build -c local",
    "build:all:development": "nx run-many --all -t build -c development",
    "build:all:staging": "nx run-many --all -t build -c staging",
    "build:all:production": "nx run-many --all -t build -c production",
    "build": "npm run build:all",
    "build:local": "npm run lint:fix && npm run build:all:local",
    "build:development": "npm run lint:fix && npm run build:all:development",
    "build:staging": "npm run lint:fix &&  npm run build:all:staging",
    "build:production": "npm run lint:fix && npm run build:all:production",
    "build:dealer-portal-theme": "nx run dealer-portal-theme:build",
    "build:polaris-ui": "nx run polaris-ui:build",
    "use-nvmrc": "nvm use $(Get-Content .nvmrc)",
    "start": "npm run lint:fix && npm run run:all:apps",
    "communications": "nx lint communications --fix=true && nx serve communications",
    "communications-admin": "nx lint communications-administration --fix=true && nx serve communications-administration",
    "user-admin": "nx lint user-administration --fix=true && nx serve user-administration",
    "user-admin-internal": "nx lint user-administration-internal --fix=true && nx serve user-administration-internal",
    "account-search": "nx lint account-search --fix=true && nx serve account-search",
    "watch": "nx build --watch",
    "test": "nx test",
    "test:headless": "nx test --browsers ChromeHeadless --no-watch --no-progress",
    "test:coverage": "nx test --no-watch --code-coverage --reporters json --browsers ChromeHeadless",
    "test:report": "run-s test:coverage test:report:generate",
    "test:report:generate": "node scripts/generate-test-coverage-report.mjs",
    "lint": "nx run-many --all -t lint",
    "lint:fix": "nx run-many --all --fix=true -t lint",
    "lint-pre-commit": "lint-staged",
    "generate:app": "node scripts/generate-app.js",
    "prepare": "husky",
    "storybook:build": "npm run storybook:prepare && nx run polaris-ui:build-storybook",
    "storybook:serve": "npm run storybook:prepare && nx run polaris-ui:storybook",
    "storybook:prepare": "npm run storybook:build:deps && npm run storybook:copy:deps && npm run storybook:extract:icons",
    "storybook:build:deps": "npm run build:dealer-portal-theme && npm run build:polaris-ui",
    "storybook:copy:deps": "rimraf packages/polaris-ui/.storybook/theme && shx mkdir -p packages/polaris-ui/.storybook/theme && shx cp -R dist/packages/dealer-portal-theme/* packages/polaris-ui/.storybook/theme",
    "storybook:extract:icons": "tsc packages/polaris-ui/scripts/extract-icons.ts --module commonjs --outDir packages/polaris-ui/scripts && node packages/polaris-ui/scripts/extract-icons.js",
    "compodoc:build": "npm run compodoc --prefix packages/polaris-ui"
  },
  "dependencies": {
    "@angular/animations": "19.2.4",
